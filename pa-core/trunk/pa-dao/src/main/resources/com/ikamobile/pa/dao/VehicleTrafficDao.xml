            <if test="week != null">
            week,
            </if>
            <if test="limitNumber != null">
            limit_number,
            </if>
            <if test="createTime != null">
            create_time,
            </if>
            <if test="updateTime != null">
            update_time,
            </if>
        </trim>
        <trim prefix="VALUES (" suffix=")" suffixOverrides=",">
            <if test="id != null">
            #{id},
            </if>
            <if test="week != null">
            #{week},
            </if>
            <if test="limitNumber != null">
